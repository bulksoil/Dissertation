pages = {859--68},
pmid = {19190011},
publisher = {Oxford University Press},
title = {{Starch storage in the stems of wheat plants: localization and temporal changes.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19190011 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC2707892},
volume = {103},
year = {2009}
}
