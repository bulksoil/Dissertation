publisher = {Public Library of Science},
title = {{Mechanical Stress Induces Biotic and Abiotic Stress Responses via a Novel {\textless}italic{\textgreater}cis{\textless}/italic{\textgreater}-Element}},
url = {http://dx.plos.org/10.1371{\%}2Fjournal.pgen.0030172},
volume = {3},
year = {2007}
}
