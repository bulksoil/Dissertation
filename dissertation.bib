issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Adipocytes,Adipocytes: metabolism,Adipose Tissue,Adipose Tissue: anatomy {\&} histology,Adipose Tissue: metabolism,Angiopoietins,Animals,Biological,Blood Proteins,Blood Proteins: deficiency,Blood Proteins: genetics,Blood Proteins: metabolism,Dietary Fats,Dietary Fats: metabolism,Digestive System,Digestive System: metabolism,Digestive System: microbiology,Energy Metabolism,Germ-Free Life,Humans,Inbred C57BL,Insulin Resistance,Insulin Resistance: physiology,Knockout,Lipoprotein Lipase,Lipoprotein Lipase: antagonists {\&} inhibitors,Liver,Liver: metabolism,Male,Mice,Models,Molecular Sequence Data,Obesity,Obesity: etiology,Triglycerides,Triglycerides: biosynthesis},
